#include <math.h>
#include <stdio.h>
#include <string.h>

int main() {
    char hh, mm, ss;
    char t12[2];
    scanf("%c:%c:%c%s", &hh, &mm, &ss, t12) ;
    printf("%c:%c:%c%s,hh,mm,ss,t12");
    if (strcmp(t12,"PM")==0 && hh!=12) hh =hh+ 12 ;
    if (strcmp(t12,"AM")==0 && hh==12) hh = 0 ;

    printf("%02c:%02c:%02c", hh, mm, ss) ;
    return 0;
}

